{
  "name": "simulation_service",
  "version": "1.0.0",
  "description": "Preforms a simulation of two AI implementations",
  "author": "Frank B Greco Jr <frankgreco@northwesternmutual.com>",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules",
    "dev": "concurrently -p \"[{name}]\" -n \"NODE INSPECTOR,NODEMON\" -c \"bgBlue.bold,bgGreen.bold\" \"node-inspector --web-port=8082 --debug-port=5859 --preload\" \"cross-env NODE_ENV=development nodemon ./node_modules/babel-cli/bin/babel-node.js --max-old-space-size=512 --debug=5859 ./index.js\"",    
    "start": "NODE_ENV=production ./node_modules/babel-cli/bin/babel-node.js ./index.js",
    "lint:dev": "node ./node_modules/eslint/bin/eslint.js . -f stylish --ext .js --cache --color",
    "lint:prod": "node ./node_modules/eslint/bin/eslint.js . --ext .js --color",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t ai_challenge/simulator-service ."
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/northwesternmutual/ai-challenge"
  },
  "bugs": {
    "url": "https://github.com/northwesternmutual/ai-challenge/issues"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "eslint": "^3.7.1",
    "node-inspector": "^0.12.8",
    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.6.0",
    "body-parser": "^1.15.0",
    "cross-env": "^3.1.2",
    "express": "^4.13.4",
    "http-response-codes": "^1.0.4",
    "memwatch-next": "^0.2.10",
    "request": "^2.72.0"
  }
}
